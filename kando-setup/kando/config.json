{
  "version": "2.1.2",
  "locale": "auto",
  "showIntroductionDialog": true,
  "menuTheme": "minecraft",
  "darkMenuTheme": "minecraft",
  "menuThemeColors": {},
  "darkMenuThemeColors": {
    "neon-lights": {
      "glow-color": "rgb(119 102 249 / 0.75)",
      "connector-color": "rgb(8 0 255 / 0.75)"
    }
  },
  "enableDarkModeForMenuThemes": true,
  "soundTheme": "none",
  "soundVolume": 0.5,
  "enableVersionCheck": true,
  "ignoreWriteProtectedConfigFiles": false,
  "settingsWindowColorScheme": "system",
